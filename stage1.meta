

Complex {
    real = 0.0
    imag = 0.0
}

init(Complex self, double real, imag) {
    self.real = real;
    self.imag = imag;
}

cmp(Complex self, other = Complex(0.0, 0.0)) int {
    if self.real == other.real
        return cmp(self.imag, other.imag)
    return cmp(self.imag, other.imag)
}

wrap(int i) Complex {
    return Complex(real=i)         // implicit convertion
}

unpack(Complex self) double, double{
    return self.real, self.imag
}

cmp(Complex self, other) int {
    if self.a == other.a
        return 0
    return self.a < other.a
}

+(Complex self, other) Complex {
    self.real, self.imag += other.real, other.imag
    return self
}

